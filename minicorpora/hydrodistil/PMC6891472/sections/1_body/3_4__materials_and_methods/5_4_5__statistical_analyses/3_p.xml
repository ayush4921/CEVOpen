        <mi>θ</mi>
        <mn>2</mn>
       </msub>
       <mo>+</mo>
       <mi>X</mi>
      </mrow>
     </mfrac>
     <mo>+</mo>
     <mi>ε</mi>
    </mrow>
   </mrow>
  </math>
 </disp-formula>
