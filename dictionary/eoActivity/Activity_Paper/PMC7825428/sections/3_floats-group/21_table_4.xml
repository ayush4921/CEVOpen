    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">[
     <xref rid="B147-foods-10-00106" ref-type="bibr">147</xref>,
     <xref rid="B148-foods-10-00106" ref-type="bibr">148</xref>] 
