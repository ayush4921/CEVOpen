      <mo>=</mo>
      <mfrac>
       <mrow>
        <mtext>Urinary </mtext>
        <msup>
         <mrow>
          <mtext>Na</mtext>
         </mrow>
         <mrow>
          <mo>+</mo>
         </mrow>
        </msup>
        <mo>,</mo>
        <msup>
         <mrow>
          <mtext>K</mtext>
