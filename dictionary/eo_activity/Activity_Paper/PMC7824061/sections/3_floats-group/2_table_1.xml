    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">n.m.</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">[
     <xref rid="B44-plants-10-00063" ref-type="bibr">44</xref>]
    </td>
   </tr>
   <tr>
    <td rowspan="2" align="center" valign="middle" style="border-bottom:solid thin" colspan="1">
     <italic>Curcuma phaeocaulis</italic>
    </td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">Rhizome</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">Methanol</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">500 mg/kg</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">6</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">Adjuvant arthritis model mice</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">Reduction in serum haptoglobin concentration</td>
    <td align="center" valign="middle" style="border-bottom:solid thin" rowspan="1" colspan="1">[
     <xref rid="B45-plants-10-00063" ref-type="bibr">45</xref>]
